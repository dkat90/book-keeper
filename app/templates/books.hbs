<div class="col-md-6 col-md-offset-3">
  {{outlet}}
  <section>
    <h2>Book List</h2>
    {{#each model as |book|}}
    <p>
      <span><strong>{{book.title}}</strong> - <em>{{book.author}}</em> - {{book.pubYear}}</span>
      <button class="btn btn-danger" {{action "deleteBook" book}}>Delete</button>
    </p>
    {{/each}}
  </section>
</div>  
